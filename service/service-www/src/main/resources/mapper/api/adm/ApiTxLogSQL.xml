<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mgkim.framework.online.api.adm.apitxlog.mapper.ApiTxLogMapper">

  <select id="selectLogList" parameterType="KCmmVO" resultType="CmmApiTxLogVO">
    SELECT A.*
      FROM MGCB407TA A /* MGCB_api처리로그arch */
     ORDER BY FIRST_REGI_DTTM DESC
  </select>

  <select id="selectLogList-count" parameterType="KCmmVO" resultType="CmmApiTxLogVO">
    SELECT COUNT(*)
      FROM (SELECT BB.*
              FROM MGCB407TA BB /* MGCB_api처리로그arch */)
  </select>

</mapper>